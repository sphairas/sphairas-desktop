<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE filesystem PUBLIC "-//NetBeans//DTD Filesystem 1.2//EN" "http://www.netbeans.org/dtds/filesystem-1_2.dtd">
<filesystem>
    <folder name="Editors">
        <folder name="AnnotationTypes">
            <file name="betula-remote-reports-save-report.xml" url="editor/betula-remote-reports-save-report.xml"/>
        </folder>                
        
        <folder name="text">
            <folder name="betula-remote-reports">
                <file name="EditorKit.instance">
                    <!--                    <attr name="instanceClass" stringvalue="org.netbeans.modules.editor.NbEditorKit"/>-->
                    <attr name="instanceClass" stringvalue="org.thespheres.betula.adminreports.editor.Kit"/>
                </file>
                <folder name="SideBar">
                    <file name="org-netbeans-modules-editor-fold-CodeFoldingSidebar.instance">
                        <attr name="location" stringvalue="west"/>
                        <!--attr supported west (default),east,south,nord scrollable (default = true), scrollable & south, east unsupported-->
                        <!--should be positioned after the current registrations, so they prevail--> 
                        <attr name="position" intvalue="1600"/>
                        <attr name="instanceof" stringvalue="org.netbeans.spi.editor.SideBarFactory"/>
                        <attr name="instanceCreate" newvalue="org.netbeans.modules.editor.fold.ui.CodeFoldingSideBar$Factory"/>
                    </file>
                </folder>
                <!--                <folder name="FontsColors">
                    <folder name="NetBeans">
                        <folder  name="Defaults">
                            <file name="org-netbeans-modules-editor-highlight-colorings.xml" url="reportsui/org-netbeans-modules-editor-highlight-colorings.xml">
                                <attr name="SystemFileSystem.localizingBundle" stringvalue="org.thespheres.betula.admin.units.Bundle"/>
                                <attr name="nbeditor-settings-ColoringType" stringvalue="highlight"/>
                            </file>
                        </folder>
                    </folder>
                </folder>-->
                
                <folder name="Preferences">
                    <folder name="Defaults">
                        <file name="betula-remote-reports-Preferences.xml" url="editor/betula-remote-reports-Preferences.xml"/>
                    </folder>
                </folder>
            </folder>
        </folder>
    </folder>
</filesystem>
